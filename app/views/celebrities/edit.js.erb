  chart_id = $('#celeb-<% c.id %>');
  chart_id2 = document.getElementById("celeb-<%= c.id %>");
  console.log('<%= c.first_name %>');
<% end %>


d3.select(chart_id2).selectAll("div").data(graph_data).enter().append("div").style("width", function(d) { return Math.max(d * chart_width, 25) + "px"; }).text(function(d) { return d*100 + '%'; });


